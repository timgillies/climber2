<br>
<div class="top-10 row">
  <div class="header bg-top-10">
    <h1>Top Sends</h1>
  </div>
<ul>
  <% Tick.top_ten(@facility).each do |send| %>
    <%= link_to user_path(send.user_id) do %>
      <li>
        <strong><%= send.grade.grade %></strong> <%= send.user.name %>
      </li>
    <% end %>
  <% end %>
</ul>
</div>
