<div id="quick_project_<%= f.id %>" class="circle-wrapper">


    <%= button_to quick_comp_project_facility_competition_route_path(@facility, @competition, f), method: :post, class: 'btn btn-pretick btn-md', remote: true,
    data: { disable_with:
      "<div class='tick-circle-fill'>
        <i class='fa fa-refresh fa-spin'></i>
      </div>"

    }  do %>
    <% unless Tick.where(competition_id: @competition.id).send_type_count(f, current_user, "project") > 0 %>
      <div class="tick-circle">
    <% else %>
      <div class="tick-circle-fill">
    <% end %>
        <%= Tick.send_type_count(f, current_user, "project") %>
        <span class="text"><%= "attempt".pluralize(Tick.where(competition_id: @competition.id).send_type_count(f, current_user, "project")) %></span>
      </div>
    <% end %>

</div>
