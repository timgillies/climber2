<table class="table table-striped table-condensed">
  <tr>
    <th>Discipline</th>
    <th>Rank</th>
    <th>Grade</th>
    <th></th>
  </tr>
    <% @facility.grades.each do |grade| %>
    <tr>
      <td><%= grade.discipline %></td>
      <td><%= grade.rank %></td>
      <td><%= grade.grade %></td>
      <td><%= link_to "edit",  edit_admin_facility_grade_path(@facility, grade ) %></td>
   </tr>
  <% end %>
</table>
</div>
