<% provide(:title, 'Add routes') %>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 bg-gray">
      <div class="form-gray">
        <h1>Route Information</h1>
      </div>
      <% if(params[:name]) %>
      <%= render 'admin/routes/prepopulated_form', object: @routes %>
      <% else %>
      <%= render 'admin/routes/form', object: @routes %>
      <% end %>
    </div>
  </div>
</div>
