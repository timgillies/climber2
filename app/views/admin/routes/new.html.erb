<% provide(:title, 'Add routes') %>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-6">
      <div class="routes">
        <h1><small>Add a route</small></h1>
      </div>
        <%= render 'admin/routes/form', object: @routes %>
    </div>
    <div class="col-sm-6">
      <div class="routes">
        <h1><small>Recently added</small></h1>
      </div>
      <%= render 'admin/routes/recent', object: @recentroutes %>
    </div>
  </div>
</div>
