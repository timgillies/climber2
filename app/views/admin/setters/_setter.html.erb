
  <% @setters.each do |f| %>
  <ol class="routelist">
    <li>
      <div class="container-fluid">
        <div class="row">
          <div class="col-sm-4">
            <p><%= f.last_name + ", " + f.first_name %></p>
          </div>
          <div class="col-sm-6">
              <p><%= f.routes.current.count %> active routes</p>
          </div>
          <div class="col-xs-1 col-sm-1 col-md-1">
            <p><%= link_to edit_admin_facility_setter_path(@facility, f ) do %>
                        <i class="glyphicon glyphicon-cog"></i>
                        <%end %>
            </p>
          </div>

          <div class="col-xs-1 col-sm-1 col-md-1">
            <p><%= link_to admin_facility_setter_path(@facility, f), data: {:confirm => 'Are you sure? This cannot be undone.'},
                        :method => :delete do %>
                        <i class="glyphicon glyphicon-trash"></i>
                        <%end %>
            </p>
          </div>
        </div>
      </div>
    </li>
  </ol>
  <% end %>
